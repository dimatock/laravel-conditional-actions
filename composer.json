{
    "name": "my-com/laravel-conditional-actions",
    "version": "1.0.0-alpha.1",
    "authors": [
        {
            "name": "Aleksandr Paramonov",
            "email": "a.paramonov@corp.my.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ConditionalActions\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        },
        "files": ["./tests/helpers.php"]
    },
    "require": {
        "illuminate/support": "^5.6",
        "illuminate/database": "^5.6",
        "illuminate/config": "^5.6",
        "illuminate/http": "^5.6",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": "ConditionalActions\\ConditionalActionsServiceProvider"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.6",
        "mockery/mockery": "^1.2"
    }
}
